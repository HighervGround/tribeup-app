{
  "buildCommand": "npm ci && npm run build",
  "installCommand": "npm ci",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!assets).*)",
      "destination": "/"
    }
  ],
  "outputDirectory": "build",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
