{
  "name": "tribeup",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "TribeUp - Social Sports App for Finding and Joining Pickup Games",
  "keywords": [
    "sports",
    "social",
    "react",
    "typescript",
    "mobile-first"
  ],
  "author": "TribeUp Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/tribeup.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:a11y": "jest --config jest.a11y.config.js",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist/stats.json",
    "preview:https": "vite preview --https",
    "check-outdated": "npm outdated"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.56.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.294.0",
    "motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.55.0",
    "react-router-dom": "^6.20.1",
    "sonner": "^2.0.3",
    "tailwind-merge": "^2.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@axe-core/react": "^4.8.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "eslint": "^8.53.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^23.0.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
